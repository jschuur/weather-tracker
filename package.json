{
  "name": "weather-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "schema:gen": "drizzle-kit generate:sqlite --out migrations --schema src/db/schema.ts",
    "schema:migrate": "tsx util/dbMigrate.ts",
    "schema:migrate:prod": "dotenv -e .env.production -- tsx util/dbMigrate.ts",
    "seed": "tsx util/seed.ts",
    "seed:prod": "dotenv -e .env.production -- tsx util/seed.ts",
    "update:metrics": "tsx util/updateMetrics.ts",
    "update:metrics:prod": "dotenv -e .env.production -- tsx util/updateMetrics.ts",
    "tb:ws:dev": "tb workspace use weathertracker_dev",
    "tb:ws:prod": "tb workspace use weathertracker_prod",
    "tb:push": "tb push --folder tinybird --force"
  },
  "dependencies": {
    "@chronark/zod-bird": "^0.2.2",
    "@heroicons/react": "^2.0.18",
    "@libsql/client": "^0.3.6",
    "@tanstack/react-query": "^5.0.5",
    "@tanstack/react-query-devtools": "^5.1.0",
    "@tanstack/react-query-next-experimental": "^5.0.5",
    "@tremor/react": "^3.10.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "lodash": "^4.17.21",
    "next": "13.5.6",
    "next-usequerystate": "^1.8.4",
    "openweather-api-node": "^3.1.2",
    "pluralize": "^8.0.0",
    "pretty-ms": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.0.3",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.8",
    "@types/pluralize": "^0.0.32",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.52.0",
    "eslint-config-next": "13.5.6",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.4",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
